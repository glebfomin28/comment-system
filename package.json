{
  "name": "vite-bs-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true --config vite.config.ts tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "eslint:check": "eslint ./src --ext ts,tsx",
    "eslint:fix": "eslint ./src --ext ts,tsx --fix",
    "stylelint:check": "stylelint src/**/*.{scss,css}",
    "stylelint:fix": "stylelint src/**/*.{scss,css} --fix",
    "prettier:check": "prettier --check \"**/**\"",
    "prettier:fix": "prettier --check \"**/**\" --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.6",
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.2.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^22.13.5",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^3.0.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gitmoji-autotag": "^0.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.4.14",
    "vite-bundle-analyzer": "^0.16.0",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^3.0.6"
  }
}
